
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/ar934x_wmac'
	option htmode 'HT20'
	option channel '6'
	option hwmode '11ng'
	option txpower '17'
	option country 'DE'
	option disabled '1'

config wifi-iface
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'CLAGE-HS-36D2'
	option encryption 'psk2'
	option key 'Warmwasser'

